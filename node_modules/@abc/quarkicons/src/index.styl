.header
  color: #333

.iconList
  margin: 20px 60px

.containerCenter
  display: flex
  flex-wrap: wrap
  justify-content: center

.icon
  align-items: center
  font-size: 4rem
  width: 120px
  display: flex
  flex-direction: column
  padding: 20px
  margin: 15px
  border: solid 1px #dadada
  background-color: #fcfcfc

  & > p
    align-items: center
    color: #8b8d92
    display: flex
    font-size: 1rem
    margin-bottom: 0
    margin-top: 20px
    text-align: center
